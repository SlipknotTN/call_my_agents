        {
            "name": "Depth model Distill any depth",
            "type": "debugpy",
            "request": "launch",
            "program": "tools/depth_model.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--image_path",
                "data/cat.jpg",
                "--hf_model_url",
                "xingyang1/Distill-Any-Depth-Small-hf",
                "--model_family",
                "distill",
                "--output_dir",
                "data/out_cat_depth_distill"
