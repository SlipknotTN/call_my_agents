            "justMyCode": false,
            "args": [
                "--image_path",
                "data/people_walking.png",
                "--model_path",
                "yolo11s-seg.pt",
                "--show_pred",
                "--output_path",
                "data/out_people_walking_ultra/bbox+mask.jpg",
                "--output_viz_format",
                "ultralytics"
            ]
        },
